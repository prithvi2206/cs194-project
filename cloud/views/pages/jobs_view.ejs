<!-- views/pages/jobs.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
  <% include ../partials/head %>
</head>

<body>
  <div id="wrapper">

    <header>
      <% include ../partials/header %>
    </header>

    <main>
      <div id="page-wrapper">

        <div class="container-fluid">

          <!-- Page Heading -->
          <div class="row">
            <div class="col-lg-12">
              <h1 class="page-header">
                <%= job.get("company") %> <small><%= job.get("title") %></small>
              </h1>
              <ol class="breadcrumb">
                <li>
                  <i class="fa fa-briefcase"></i> 
                  <a href="/jobs">Jobs</a>
                </li>
                <li class="active">
                  <i class="fa"></i> View Application
                </li>
              </ol>
            </div>
          </div>

        </div>
        <!-- /.container-fluid -->
      </div>
    </main>

  </div>


  <footer>
    <% include ../partials/footer %>
  </footer>

</body>
</html>

